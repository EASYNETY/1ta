{
	"name": "1techacademy-frontend",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "cross-env NODE_OPTIONS=\"--openssl-legacy-provider --max-old-space-size=2048 --expose-gc\" next dev",
		"build": "cross-env NODE_OPTIONS=\"--openssl-legacy-provider --max-old-space-size=4096\" next build",
		"build:safe": "cross-env NODE_OPTIONS=\"--openssl-legacy-provider --max-old-space-size=6144\" next build",
		"build:clean": "rimraf .next out && npm run build",
		"start": "cross-env NODE_OPTIONS=\"--openssl-legacy-provider --max-old-space-size=1024 --expose-gc\" next start",
		"start:memory-safe": "cross-env NODE_OPTIONS=\"--openssl-legacy-provider --max-old-space-size=1024 --expose-gc --optimize-for-size --gc-interval=100\" next start",
		"lint": "next lint",
		"install:safe": "npm install --legacy-peer-deps --ignore-engines",
		"install:pnpm": "pnpm install",
		"install:pnpm:force": "pnpm install --force",
		"dev:pnpm": "pnpm run dev",
		"build:pnpm": "pnpm run build",
		"memory:monitor": "node scripts/memory-monitor.js start",
		"memory:report": "node scripts/memory-monitor.js report",
		"memory:gc": "node scripts/memory-monitor.js gc",
		"memory:clean": "node scripts/memory-monitor.js clean",
		"pm2:start": "pm2 start ecosystem.config.js",
		"pm2:restart": "pm2 restart ecosystem.config.js",
		"pm2:stop": "pm2 stop ecosystem.config.js",
		"pm2:logs": "pm2 logs",
		"prepare": "node -e \"if (process.env.VERCEL !== '1') { require('child_process').execSync('npx husky install', { stdio: 'inherit' }); }\""
	},
